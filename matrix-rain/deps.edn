
{:paths ["src" "driver" "repl"]

 :deps
 {org.clojure/clojure {:mvn/version "1.12.4"}
  quil/quil           {:mvn/version "4.3.1563"}}

 :aliases
 {:dev
  {:extra-deps
   {nrepl/nrepl       {:mvn/version "1.4.0"}
    cider/cider-nrepl {:mvn/version "0.58.0"}}
   :main-opts
   ["-m" "nrepl.cmdline"
    "--middleware" "[cider.nrepl/cider-middleware]"
    "--interactive"]}

  :build
  {:deps
   {io.github.clojure/tools.build {:mvn/version "0.10.11"}}
   :paths ["build"]
   :ns-default build}}}
